{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Windjammer Editor",
  "version": "0.1.0",
  "identifier": "org.windjammer.editor",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:8080",
    "frontendDist": "./ui"
  },
  "bundle": {
    "active": false,
    "targets": "all"
  },
  "app": {
    "windows": [
      {
        "title": "Windjammer Editor",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
