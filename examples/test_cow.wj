// Test Phase 9: Cow (Clone-on-Write) optimization
// This example shows conditional string modification

fn process_text(text: string, uppercase: bool) -> string {
    if uppercase {
        text.to_uppercase()
    } else {
        text
    }
}

fn main() {
    let result1 = process_text("hello", true);
    println!("{}", result1);
    
    let result2 = process_text("world", false);
    println!("{}", result2);
}
