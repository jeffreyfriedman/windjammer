// Test: Vec indexing through immutable reference should use &, not &mut

pub struct Container {
    pub items: Vec<i64>,
}

pub fn read_item(container: &Container, index: i64) -> i64 {
    container.items[index as int]
}

pub fn main() {
    let mut c = Container { items: Vec::new() }
    c.items.push(42)
    
    let value = read_item(&c, 0)
    println!("Value: {}", value)
}

