// Test: Module-qualified types should generate correct import paths
// This is a simplified test focused on import generation

pub mod math {
    pub struct Vec2 {
        pub x: f32,
        pub y: f32,
    }
}

pub struct Sprite {
    pub position: math::Vec2,
}

impl Sprite {
    pub fn new() -> Sprite {
        Sprite {
            position: math::Vec2 { x: 0.0, y: 0.0 },
        }
    }

    pub fn set_position(pos: math::Vec2) {
        self.position = pos
    }
}

pub fn main() {
    let mut sprite = Sprite::new();
    let new_pos = math::Vec2 { x: 10.0, y: 20.0 };
    sprite.set_position(new_pos);
    println!("Position: {}, {}", sprite.position.x, sprite.position.y);
}

