use crate::math::Vec3
use std::collections::HashMap

/// Path result
pub struct NavmeshPath {
    pub waypoints: Vec<Vec3>,
}

impl NavmeshPath {
    pub fn new() -> NavmeshPath {
        NavmeshPath {
            waypoints: Vec::new(),
        }
    }

    pub fn len(self) -> i32 {
        self.waypoints.len() as i32
    }
}

fn main() {
    let path = NavmeshPath::new()
    let count = path.len()
    println("Path has {} waypoints", count)
