// Test: Basic struct declaration and methods

pub struct Point {
    pub x: f32,
    pub y: f32,
}

impl Point {
    pub fn new(x: f32, y: f32) -> Point {
        Point { x, y }
    }
    
    pub fn distance_from_origin(self) -> f32 {
        (self.x * self.x + self.y * self.y).sqrt()
    }
}


























